# Environment Configuration Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env file to version control

# API Configuration
API_BASE_URL=https://api.yourapp.com
API_TIMEOUT=30000

# Authentication
# For production, use a secure backend authentication service
AUTH_TOKEN_KEY=@habit_tracker_auth_token
REFRESH_TOKEN_KEY=@habit_tracker_refresh_token

# Analytics (Optional)
# ANALYTICS_API_KEY=your_analytics_key_here
# SENTRY_DSN=your_sentry_dsn_here

# Feature Flags
ENABLE_PREMIUM_FEATURES=false
ENABLE_ANALYTICS=false

# App Configuration
APP_VERSION=1.0.0
APP_ENV=development

# Security
# Maximum login attempts before lockout
MAX_LOGIN_ATTEMPTS=5
# Lockout duration in milliseconds (15 minutes)
LOGIN_LOCKOUT_DURATION=900000

# Note: Sensitive values should NEVER be hardcoded
# Use a secure backend for storing API keys and secrets
# This file is for configuration only, not for sensitive credentials
